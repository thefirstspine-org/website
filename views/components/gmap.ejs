<script>
  Vue.component('gmap', {
    props: ['latitude', 'longitude', 'gkey'],
    template: '#gmap',
  })
</script>

<script type="text/x-template" id="gmap">
  <div>
    <iframe
      style="width:100%;height:100%"
      :src="`https://www.google.com/maps/embed/v1/search?key=${gkey}&q=${latitude},${longitude}`"></iframe>
  </div>
</script>
